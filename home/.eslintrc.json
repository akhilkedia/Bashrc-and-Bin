{
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "rules": {
        "brace-style": "off",
        "camelcase": "warn",
        "constructor-super": "warn",
        "max-len": "off",
        "new-cap": "off",
        "no-console": "off",
        "no-constant-condition": "off",
        "no-multi-spaces": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "require-jsdoc": "off",
        "valid-typeof": "warn"
    }
}